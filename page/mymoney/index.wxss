page{
  width: 100%;
  height: 100%;
  background: #eee;
}
.wrapper{
  width: 100%;
  height: 100%;
}
.top{
  width: 100%;
  text-align: center;
  padding: 50rpx 0;
  background-color: white;
}
.top text{
  display: block;
  margin-top: 60rpx;
  font-size: 25rpx;
}
.top text:nth-child(1){
  margin-top: 0;
}
.top text:nth-child(2){
  font-size: 100rpx;
  font-weight: 600;
}
.top text:nth-child(3){
  width:20%;
  border: 1px solid black;
  margin: 60rpx auto 0;
  border-radius: 10rpx;
  padding: 6rpx 0;
}
.main{
  width: 100%;
  padding: 30rpx 0;
}
.main button{
  width: 90%;
  background-color: #b9dd08;
  color: black;
  font-weight: 600;
}
.foot{
  width: 100%;
  padding: 30rpx 50rpx;
  background-color: white;
  font-size: 30rpx;
}
.foot view{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  position: relative;
}
.foot view .yi{
  position: absolute;
  right: 90rpx;
}
.foot view .er{
  position: absolute;
  right: 90rpx;
  color: #b9dd08;
}
.foot view .san{
  color: black;
}
.info{
  width: 100%;
  padding: 30rpx 50rpx;
  background-color: white;
  font-size: 30rpx;
  margin-top: 50rpx;
  position: relative;
}
.info text:nth-child(2){
  position: absolute;
  right: 140rpx;
}